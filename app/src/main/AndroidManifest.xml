        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackTutorial">
        <activity
            android:name=".twowaydatabinding.TwoWayOneActivity"
